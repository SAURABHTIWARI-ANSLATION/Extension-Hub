{
  "manifest_version": 3,
  "name": "Icon Finder from Website",
  "description": "Extract SVG and image icons from any website.",
  "version": "1.1",
  "permissions": ["activeTab", "scripting", "downloads"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}