{
  "manifest_version": 3,
  "name": "Keyword Extractor",
  "version": "1.0",
  "description": "Extract top keywords from any page or text",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
